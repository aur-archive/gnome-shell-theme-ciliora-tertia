pkgname=gnome-shell-theme-ciliora-tertia
pkgver=1.0
pkgrel=1
pkgdesc="Ciliora Tertia GNOME Shell theme."
arch=('any')
url="http://zagortenay333.deviantart.com/art/Ciliora-Tertia-Shell-536134191"
license=('GPLv3')
depends=('gnome-shell')
source=('http://orig05.deviantart.net/bd6d/f/2015/152/b/5/ciliora_tertia_shell_by_zagortenay333-d8v77sf.zip')
sha256sums=('SKIP')

package() {
  install -dm755 ${pkgdir}/usr/share/themes
  cp -r Ciliora-Tertia ${pkgdir}/usr/share/themes
} 
